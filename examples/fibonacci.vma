# Compute fibonacci sequence

main:
	str 44 rd # 44th number is the highest i32 can handle

	str 1 ra  # a
	str 1 rb  # b


fibonacci:

	jz rd end

	add ra rb rc # c = a + b

	#TODO fix this!
	add rb re ra # a = b + 0 
	add rc re rb # b = c + 0

	out rc

	str 1 rf # let count = count - 1
	sub rd rf rd

	jmp fibonacci
end:
	hlt